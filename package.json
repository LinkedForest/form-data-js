{
  "name": "form-data-js",
  "version": "1.0.0",
  "description": "Middleware Data Handler TypeScript Library To FormData Type Output.",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "files": ["dist"],
  "scripts": {
    "build": "rm -rf dist/ && prettier --write src/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LinkedForest/form-data-js.git"
  },
  "keywords": [
    "Node", "React", "Angular", "Form", "Data", "JSON", "Submit", "FormData", "TypeScript"
  ],
  "author": "ramy-moustafa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LinkedForest/form-data-js/issues"
  },
  "homepage": "https://github.com/LinkedForest/form-data-js#readme",
  "devDependencies": {
    "@types/node": "^18.11.13",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
